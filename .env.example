KEYROCK_CLIENT_SECRET=your-client-secret
KEYROCK_APP_ID=your-backend-app-id
KEYROCK_CALLBACK_URL=http://localhost:3000/auth/callback

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=24h

# PEP Proxy (Wilma) Configuration
PEP_PROXY_URL=http://localhost:1027

# IoT Agent Configuration
IOT_AGENT_URL=http://localhost:4041

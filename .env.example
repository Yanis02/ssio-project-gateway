# If not set, south port URL is derived from IOT_AGENT_URL (port 4041 -> 7896)
IOT_AGENT_SOUTH_PROXY_URL=http://localhost:7897

# FIWARE Service Configuration
FIWARE_SERVICE=openiot
FIWARE_SERVICE_PATH=/